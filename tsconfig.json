{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "assets/*": [
        "src/assets/*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "context/*": [
        "src/context/*"
      ],
      "screens/*": [
        "src/screens/*"
      ],
      "stack/*": [
        "src/stack/*"
      ],
      "testData/*": [
        "src/testData/*"
      ],
      "theme/*": [
        "src/theme/*"
      ],
      "domain/*": [
        "src/domain/*"
      ]
    }
  }
}
